    @{
        var counter = 1;
    }
    @foreach (var p in Model.Products)
    {
        var m = p.MastProduct;
        <tr>
            <td>@(counter++)</td>
            <td>@p.cprod_code1</td>
            <td>@p.cprod_range</td>
            <td>@p.cprod_name</td>
            <td></td>
