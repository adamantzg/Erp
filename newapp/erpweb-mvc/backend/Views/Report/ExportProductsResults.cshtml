<div class="container-fluid">
    <div class="row">
        <table style="width: 800px">
            <tr>
                <td style="width: 200px" colspan="2"><img src="@(Path.Combine(Settings.Default.LogosFolder, Model.Client.clientlogo))" /></td>
                <td>
                    <h4>PRODUCT SET-UP DOCUMENT</h4>
                    <h5>RANGE DETAIL</h5>
                    @{
                        var f = Model.Factory;
                    }
                    <table class="table table-bordered" style="width: 80%">
                        <tr>
                            <td style="width: 150px" class="factorylabel">SUPPLIER NAME</td>
                            <td>@f.user_name</td>
                        </tr>
                        <tr>
                            <td class="factorylabel">SUPPLIER CODE</td>
                            <td>@f.factory_code</td>
                        </tr>
                        <tr>
                            <td class="factorylabel">SUPPLIER ADDRESS</td>
                            <td>@WebUtilities.ArrayToString(new[] { f.user_address1, f.user_address2, f.user_address3, f.user_address4, f.user_address5 }, ",")</td>
                        </tr>
                        <tr>
                            <td class="factorylabel">SUPPLIER TELEPHONE NUMBER</td>
                            <td>@f.user_tel</td>
                        </tr>
                        <tr>
                            <td class="factorylabel">SUPPLIER CONTACT PERSON</td>
                            <td>@f.user_contact</td>
                        </tr>
                        <tr>
                            <td class="factorylabel">DATE</td>
                            <td>@DateTime.Today.ToString("d")</td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>
        @*<div class="col-md-2"><img src="@(Path.Combine(Settings.Default.LogosFolder, Model.Client.clientlogo))"/>
        </div>
        <div class="col-md-10">
            
        </div>*@
