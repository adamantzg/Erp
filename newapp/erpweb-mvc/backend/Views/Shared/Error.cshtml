@model HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<div class="title">
    <h1 class="error">Error.</h1>
    <h2 class="error">An error occurred while processing your request.</h2>
    <h3 class="error">@Model.Exception.Message </h3>
    <h3 class="error">@Model.Exception.StackTrace </h3>
   
</div>
