@section head
{
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/bs/dt-1.10.15/b-1.3.1/r-2.1.1/se-1.2.2/datatables.min.css" />
    @Styles.Render("~/Content/DataTables/style/css")
    @Styles.Render("~/Content/font-awesome.css")
    @Styles.Render("~/Content/ionicons.css")
    @Styles.Render("~/Content/style.css")
    @Styles.Render("~/Content/bootstrapStyle/bootstrap")

    <link href="~/Content/loading.css" rel="stylesheet" />
    <!-- google font -->
    <link href='https://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>

    <style>
        @@media (min-width: 768px) {
            #footer {
                width: 750px
            }
        }

        @@media (min-width: 992px) {
            #footer {
                width: 970px
            }
        }

        @@media (min-width: 1200px) {
            #footer {
                width: 1170px
            }
        }

        .content {
            background: none;
        }

        .main {
            width: auto;
        }

        html, body {
            background: none;
        }

        .radio-inline > * {
            padding-top: 0px !important;
        }

        .imageHolder {
            position: relative;
            height: 230px;
            border: 1px solid black;
            display: table-cell;
            vertical-align: middle;
            width: 300px;
        }

        .orders_before_ready_date {
            border: 1px solid red;
        }

        .badge-success {
            background: #5cb85c !important
        }

        .badge-danger {
            background: #d9534f;
        }

        label {
            font-size: 1em !important;
        }

        [ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak {
            display: none !important;
        }

        .tab-pane .row {
            margin-left: 0;
            margin-right: 0;
        }
    </style>

}

<div ng-app="app">

    <div loading class="loading-dialog"></div>

    <div ui-view> </div>

</div>

@section scripts2 {
    @Html.Raw(JSNLog.JavascriptLogging.Configure())
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/ang-bootstrap")
    @Scripts.Render("~/bundles/utils")
    @Scripts.Render("~/bundles/globalize")
    @*@Scripts.Render("~/bundles/DataTable")*@
    <script type="text/javascript" src="https://cdn.datatables.net/v/bs/dt-1.10.15/b-1.3.1/r-2.1.1/se-1.2.2/datatables.min.js"></script>
    @Scripts.Render("~/bundles/AngularDataTable")
    <script src="//cdn.datatables.net/plug-ins/1.10.13/sorting/datetime-moment.js"></script>
    <script src="~/Scripts/logToServer.js"></script>


    @Scripts.Render("~/bundles/plupload2")

    <script src="~/Scripts/angular-plupload/angular-plupload.js"></script>
    <script src="~/Scripts/functions.js"></script>


    <script src="~/AngularApps/BudgetActualData/js/app.js"></script>
    <script src="~/AngularApps/BudgetActualData/js/controllers.js"></script>
    <script src="~/AngularApps/BudgetActualData/js/factory.js"></script>
    <script src="~/Scripts/loading.js"></script>


}





